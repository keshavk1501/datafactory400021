{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory400021"
		},
		"datalake4321_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake4321'"
		},
		"synapseworkspace4321_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapseworkspace4321'"
		},
		"datalake4321_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake4321.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory400021_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake4321_accountKey": {
						"value": "[parameters('datalake4321_accountKey')]"
					},
					"synapseworkspace4321_connectionString": {
						"value": "[parameters('synapseworkspace4321_connectionString')]"
					},
					"datalake4321_properties_typeProperties_url": {
						"value": "[parameters('datalake4321_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}